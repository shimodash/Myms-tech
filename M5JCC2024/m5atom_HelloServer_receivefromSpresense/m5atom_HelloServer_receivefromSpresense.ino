#include <WiFi.h>
#include <WebServer.h>
#include <ESPmDNS.h>
#include <M5Atom.h>

// 26ピンを受信、32ピンを送信として設定
#define RX_PIN 26
#define TX_PIN 32


// WiFi設定
const char *ssid = "set-your-netwoek";
const char *password = "set-your-passwd";

WebServer server(80);
const int led = 13;

// Base64形式の画像データ
const char* base64Image = "";



void handleRoot() {
  digitalWrite(led, 1);
  server.send(200, "text/plain", "hello from esp32!");
  digitalWrite(led, 0);
}

void handleNotFound() {
  digitalWrite(led, 1);
  String message = "File Not Found\n\n";
  message += "URI: ";
  message += server.uri();
  message += "\nMethod: ";
  message += (server.method() == HTTP_GET) ? "GET" : "POST";
  message += "\nArguments: ";
  message += server.args();
  message += "\n";
  for (uint8_t i = 0; i < server.args(); i++) {
    message += " " + server.argName(i) + ": " + server.arg(i) + "\n";
  }
  server.send(404, "text/plain", message);
  digitalWrite(led, 0);
}

int access_cnt = 0;
String http_string = "";
void handleInline() {
  digitalWrite(led, 1);
  // String html = "<html><body><h1>Data from Spresense</h1>";
  String html = "<html><body>";
  html += "<img src='data:image/png;base64,";
  html += base64Image;
  html += "'/><br>";
  html += "<h2>Gps Data from Spresense</h2><br>";
  html += String(access_cnt);
  html += http_string;
  html += "</body></html>";
  server.send(200, "text/html", html);
  digitalWrite(led, 0);
  access_cnt++;
}

void setup(void) {
  M5.begin(true, false, true);
  // LEDの初期化
  M5.dis.clear();
  // M5.dis.setBrightness(10); 
  for (int x = 0; x < 5; x++) {
    for (int y = 0; y < 5; y++) {
      M5.dis.drawpix(x + y * 5, 0xFF0000); // 赤色 (0xFF0000)
    }
  }

  M5.update();
  

  pinMode(led, OUTPUT);
  digitalWrite(led, 0);
  Serial.begin(115200);
  WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);
  Serial.println("");

  // ソフトウェアシリアル通信を開始
  Serial2.begin(9600, SERIAL_8N1, RX_PIN, TX_PIN);
  Serial.println("Software serial started.");
  // 接続待ち
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  Serial.println("");
  Serial.print("Connected to ");
  Serial.println(ssid);
  Serial.print("IP address: ");
  Serial.println(WiFi.localIP());

  if (MDNS.begin("esp32")) {
    Serial.println("MDNS responder started");
  }

  server.on("/", handleRoot);

  server.on("/inline", handleInline);

  server.onNotFound(handleNotFound);

  server.begin();
  Serial.println("HTTP server started");
}

String inputString = "";    // 文字列を保存するための変数
bool stringComplete = false;  // 文字列が完了したかどうかを示すフラグ

void loop(void) {

  server.handleClient();
  // ソフトウェアシリアルからのデータを読み取り、デフォルトのシリアルポートに出力
  if (Serial2.available()) {
    char inChar = (char)Serial2.read();
    inputString += inChar;
    if (inChar == '\n') {
      stringComplete = true;
    }
    access_cnt++;
    }

  if (stringComplete) {
    Serial.print("Received: ");
    Serial.print(inputString);
    for (int x = 0; x < 5; x++) {
     for (int y = 0; y < 5; y++) {
       M5.dis.drawpix(x + y * 5, 0x00FF00); // 緑色 (0x00FF00)
       M5.update();
     }
   }



    // strcpy(inputString,http_string);
    if (http_string.length() > 5000)
    {
      http_string="";
    }
   http_string=http_string + "<br>"+ inputString;

    inputString = "";
    stringComplete = false;
  }


  // デフォルトのシリアルポートからのデータをソフトウェアシリアルに送信
  if (Serial.available()) {
    char c = Serial.read();
    Serial2.write(c);
  }  
  delay(2);  // CPUが他のタスクに切り替わるのを許可
  
}
